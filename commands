//Init
npm init

//solidity compiler
npm install solc

//mocha
npm install mocha

//ganache
npm install ganache-cli

//web3
npm install web3

//truffle/hdwallet-provider
npm install @truffle/hdwallet-provider

//GIT
git init
git add .
git commit -m "Initializing Repo"
git branch -M main
git remote add origin https://github.com/adolfredo-coneo/blockchain-test.git
git push -u origin main

